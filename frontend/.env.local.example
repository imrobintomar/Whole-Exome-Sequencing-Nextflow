NEXT_PUBLIC_API_URL=https://api.atgcflow.com/

# Contact Email Addresses
NEXT_PUBLIC_CONTACT_EMAIL=aiimsgenomics@gmail.com
NEXT_PUBLIC_SUPPORT_EMAIL=aiimsgenomics@gmail.com
NEXT_PUBLIC_COLLABORATION_EMAIL=drprabudhgoel@gmail.com
